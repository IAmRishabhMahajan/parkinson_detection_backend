{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/index.py": {
      "runtime": "vercel-python@3.11",
      "maxLambdaSize": "50mb",
      "includeFiles": [
        "model_feature_analysis/**/*",
        "sessions.csv",
        "parkinson_core_services_uk.csv"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
