{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python@3.1.0",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9",
        "includeFiles": [
          "model_feature_analysis/**/*",
          "sessions.csv",
          "parkinson_core_services_uk.csv"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
}
