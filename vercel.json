{
  "version": 2,
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python@3.1.0",
      "config": {
        "maxLambdaSize": "50mb",
        "includeFiles": ["parkinson_core_services_uk.csv"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ]
}
